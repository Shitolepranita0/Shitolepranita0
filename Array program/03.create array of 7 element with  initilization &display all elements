  printf("\n second element=%d",value[1]);
  printf("\n third element=%d",value[2]);
  printf("\n fourth element=%d",value[3]);
  printf("\n fifth element=%d",value[4]);
  printf("\n sixth element=%d",value[5]);
  printf("\n seventh element=%d",value[6]);
