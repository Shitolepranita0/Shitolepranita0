void Area()
{
 int base;
 int height;
 float Area;
 printf("enter the base");
 scanf("%d",&base);
 printf("enter the height");
 scanf("%d",&height);
 Area=base*height/2;
 printf("area of triangle :%0.2f", Area);
 }
void main()
{
  clrscr();
