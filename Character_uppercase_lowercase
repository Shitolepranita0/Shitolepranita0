#include<stdio.h>
#include<conio.h>
int main()
{
  char ch='\0';
  printf("\n\t enter your character");
  scanf("%c", &ch);
  if(ch>'a'&&ch<='z')
  {
    printf("\n\t entered character is uppercase \n\t");
  }
  else
  {
    printf("\n\t entered letter is neither uppercase or lowercase");
  }
  getch();
  return 0;
}
