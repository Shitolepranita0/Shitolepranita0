#include<stdio.h>
#include<conio.h>
int main()
{
  int no=0, cnt=0;
  printf("\n enter number for reverse table=");
  scanf("%d", &no);
  printf("\n\n*****************************\n");
  for(cnt=10;cnt>=1;cnt--)
   {
     printf("\n\t%d",no*cnt);
   }
     printf("\n\n****************************\n");
     printf("\n\Thanks!!!");
     getch();
     return 0;
 };
