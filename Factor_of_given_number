#include<stdio.h>
#include<conio.h>
int main()
{
  int no=0, fact=0, temp=0;
  printf("\n number to calculate its factorial=");
  scanf("%d", &no);
  temp=no;
  fact=1;
  {
    if(fact>=0)
    printf("factorial of %d is", fact);
    temp++;
  }
   {
  printf("the factorial is %d!=%d=no, fact");
    }
  printf(" factorial of number is %d", fact);
}
