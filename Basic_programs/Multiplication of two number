#include<stdio.h>
#include<conio.h>

int main()
{
         int Num1 = 0, Num2 = 0, Mult = 0;

         printf("\n===================*********====================\n");

         printf("\n Enter First Number For Multiplication = ");
         scanf("%d",&Num1);

         printf("\n Enter Second Number FOr Multiplication = ");
         scanf("%d",&Num2);

         Mult = Num1 * Num2;

         printf("\n Multiplication Of %d * %d = %d",Num1,Num2,Mult);

         printf("\n\n===================*********===================");

         printf("\n\n\t Thanks !!!\n");

         getch();
         return 0;
}
