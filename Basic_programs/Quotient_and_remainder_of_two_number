#include<stdio.h>
#include<conio.h>
int main()
{
  int num1=0,num2=0;
  printf("\n\t enter first number=");
  scanf("%d", &num1);
  printf("\n\t enter second number=");
  scanf("%d", &num2);
  printf("\n\t quotient=%d", num1%num2);
  printf("\n\t remainder=%d", num1%num2);
  printf("\n\t\t Thanks!!!\n");
  getch();
  return 0;
}

