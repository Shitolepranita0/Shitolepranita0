#include <stdio.h>
#include<conio.h>

int main()
{
     int Num1 = 0,Num2=0,Div;
     printf("Enter first Number = ");
     scanf("%d",&Num1);

     printf("Enter Second Number = ");
     scanf("%d",&Num2);
     
     Div = Num1/Num2;
     
     printf("Div Of %d / %d = %d",Num1,Num2,Div);
      
     getch();
     return 0;
}
