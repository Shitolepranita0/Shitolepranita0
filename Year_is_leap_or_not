#include<stdio.h>
#include<conio.h>
int main()
{
   int year=0;
   printf("\n**************************\n");
   printf("\n\t enter year=");
   scanf("%d", &year);
   if(year%4==0)
   {
     printf("\n\t %d is leap year", year);
   }
   else
   {
     printf("\n\t %d is not leap year", year);
   }
    printf("\n\n**************************\n");
    getch();
    return 0;
}
